{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "blog",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "React",
        "javascript",
        "css",
        "gatsby",
        "coding",
        "programming",
        "frontend",
        "backend",
        "node",
        "express",
        "Angular"
      ]
    },
    "description": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date"
    }
  }
}
